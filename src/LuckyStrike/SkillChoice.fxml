<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="p" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LuckyStrike.SkillChoice">
   <center>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="first" mnemonicParsing="false" onAction="#setTextchoice1" text="1" />
            <Button fx:id="two" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#setTextchoice2" text="2" />
            <Button fx:id="three" layoutX="10.0" layoutY="35.0" mnemonicParsing="false" onAction="#setTextchoice3" text="3" />
            <Button fx:id="four" layoutX="10.0" layoutY="60.0" mnemonicParsing="false" onAction="#setTextchoice4" text="4" />
         </children>
      </VBox>
   </center>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <bottom>
      <HBox spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="choice" onAction="#Backtomenu" />
            <Button fx:id="confirm" mnemonicParsing="false" onAction="#Backtomenu" text="Confirm" />
         </children>
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
